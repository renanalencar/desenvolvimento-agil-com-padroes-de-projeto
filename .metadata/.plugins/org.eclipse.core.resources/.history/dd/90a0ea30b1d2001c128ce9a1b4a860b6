package br.dev.rca;

/**
 * @author renanalencar
 *
 */

/**
 * classe Achievement representa uma conquista. Cada instância deve possui uma
 * propriedade "name" que identifica o Achievement.
 *
 */
public class Achievement {

	private String name;

	public Achievement(String name) {
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
