package br.dev.rca;

import java.util.List;

/**
 * @author renanalencar
 *
 */

/**
 * Crie uma classe chamada Achievement para representar uma conquista. Cada
 * instância deve possuir uma propriedade "name" que identifica o Achievement.
 *
 */
public abstract class Achievement {

	private String name;

	public Achievement(String name) {
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public abstract void add(List<Achievement> achievements);

}
